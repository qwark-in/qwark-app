{
  "name": "qwark-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "packageManager": "pnpm@10.11.0",
  "private": true,
  "scripts": {
    "build:prod:android": "eas build --profile production --platform android",
    "build:prev:android": "eas build --profile preview --platform android",
    "build:dev:android": "eas build --profile development --platform android",
    "build:storybook:android": "eas build --profile storybook --platform android",
    "clean:all": "pnpm clean:android && pnpm clean:gradle-cache && pnpm clean:node-modules && pnpm clean:pnpm-cache && pnpm clean:npm-cache",
    "clean:android": "pnpx rimraf ./android",
    "clean:gradle-cache": "cd android && gradlew clean",
    "clean:node-modules": "pnpx rimraf --glob **/node_modules",
    "clean:npm-cache": "npm cache clean --force && npm cache verify",
    "clean:pnpm-cache": "pnpm store prune && pnpx rimraf ./pnpm-lock.json",
    "check:tamagui": "tamagui check",
    "check:expo": "expo doctor",
    "local:dev:android": "pnpm prebuild:dev:android && cross-env APP_VARIANT='development' expo run:android",
    "local:prev:android": "pnpm prebuild:prev:android && cross-env APP_VARIANT='preview' expo run:android --variant release",
    "local:prod:android": "pnpm prebuild:prod:android && cross-env APP_VARIANT='production' expo run:android --variant release",
    "local:dev:ios": "expo run:ios",
    "postbuild:prev:android": "pnpm prebuild:dev:android",
    "postbuild:prod:android": "pnpm prebuild:dev:android",
    "prebuild:prod:android": "cross-env APP_VARIANT='production' expo prebuild --platform android --clean",
    "prebuild:prev:android": "cross-env APP_VARIANT='preview' expo prebuild --platform android --clean",
    "prebuild:dev:android": "cross-env APP_VARIANT='development' expo prebuild --platform android --clean",
    "start:dev": "cross-env APP_VARIANT='development' expo start --dev-client",
    "start:storybook": "cross-env APP_VARIANT='storybook' expo start --dev-client",
    "start:web": "expo start --web",
    "svgr:all": "pnpm svgr:icons:adaptive && pnpm svgr:icons:fixed-color && pnpm svgr:logos && pnpm svgr:logos:static && pnpm svgr:illustrations",
    "svgr:icons:adaptive": "pnpm svgr --config-file config/svgr/svgr.icon.config.js --out-dir ui/assets/icons/adaptive/glyphs assets/svg/icons/adaptive",
    "svgr:icons:fixed-color": "pnpm svgr --config-file config/svgr/svgr.logo.config.js --out-dir ui/assets/icons/fixed-color/glyphs assets/svg/icons/fixed-color",
    "svgr:illustrations": "pnpm svgr --config-file config/svgr/svgr.illustration.config.js --out-dir ui/assets/illustrations assets/svg/illustrations",
    "svgr:logos": "pnpm svgr --config-file config/svgr/svgr.logo.config.js --out-dir ui/assets/bank-logos assets/svg/bank-logos",
    "svgr:logos:static": "pnpm svgr --config-file config/svgr/svgr.illustration.config.js --out-dir ui/assets/logos assets/svg/logos",
    "test": "jest",
    "upgrade:tamagui": "pnpm up '*tamagui*'@latest '@tamagui/*'@latest",
    "upgrade:tamagui:canary": "pnpm up '*tamagui*'@canary '@tamagui/*'@canary",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5.2.8",
    "@hookform/resolvers": "^5.2.2",
    "@lottiefiles/dotlottie-react": "^0.17.10",
    "@react-native-community/datetimepicker": "^8.4.4",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-firebase/analytics": "^23.7.0",
    "@react-native-firebase/app": "^23.7.0",
    "@react-native-firebase/messaging": "^23.7.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.9.2",
    "@react-navigation/material-top-tabs": "^7.4.10",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.16",
    "@shopify/react-native-skia": "^2.4.14",
    "@tamagui/config": "^1.139.3",
    "@tamagui/create-theme": "^1.141.4",
    "@tamagui/lucide-icons": "^1.139.3",
    "@tamagui/shorthands": "^1.141.4",
    "@tamagui/toast": "^1.139.3",
    "axios": "^1.13.2",
    "babel-preset-expo": "~54.0.9",
    "burnt": "^0.12.2",
    "date-fns": "^4.1.0",
    "expo": "^54.0.30",
    "expo-audio": "^1.1.1",
    "expo-build-properties": "~1.0.9",
    "expo-constants": "^18.0.12",
    "expo-crypto": "^15.0.8",
    "expo-font": "~14.0.9",
    "expo-haptics": "^15.0.8",
    "expo-linear-gradient": "^15.0.8",
    "expo-linking": "~8.0.11",
    "expo-local-authentication": "^17.0.8",
    "expo-router": "~6.0.21",
    "expo-secure-store": "^15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "idb-keyval": "^6.2.2",
    "immer": "^11.0.1",
    "lottie-react-native": "^7.3.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-gifted-charts": "^0.0.12",
    "react-hook-form": "^7.68.0",
    "react-native": "0.81.5",
    "react-native-android-sms-listener": "^0.8.0",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-get-sms-android": "^2.1.0",
    "react-native-gifted-charts": "^1.4.70",
    "react-native-keyboard-controller": "^1.18.5",
    "react-native-mmkv": "^4.1.0",
    "react-native-pager-view": "^6.9.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-webview": "^13.15.0",
    "react-native-worklets": "0.5.1",
    "react-timer-hook": "^4.0.5",
    "swr": "^2.3.7",
    "tamagui": "^1.139.3",
    "victory": "^37.3.6",
    "victory-native": "^41.20.2",
    "zod": "^4.2.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@expo/metro-runtime": "~6.1.2",
    "@svgr/cli": "^8.1.0",
    "@tamagui/babel-plugin": "^1.139.3",
    "@tamagui/cli": "^1.139.3",
    "@tamagui/metro-plugin": "^1.139.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "29.5.14",
    "@types/node": "^25.0.3",
    "@types/react": "~19.1.10",
    "cross-env": "^10.1.0",
    "handlebars": "4.7.8",
    "jest": "29.7.0",
    "jest-date-mock": "^1.0.10",
    "jest-expo": "^54.0.16",
    "ts-node": "10.9.2",
    "typescript": "~5.9.2"
  }
}
